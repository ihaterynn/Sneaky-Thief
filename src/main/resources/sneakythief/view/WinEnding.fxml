<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="720.0" prefWidth="1280.0" xmlns="http://javafx.com/javafx/8.0.401" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sneakythief.controller.WinEndingController">
    <children>
        <!-- Background Image -->
        <ImageView fitHeight="720.0" fitWidth="1280.0" preserveRatio="false">
            <image>
                <Image url="@../../Graphics/Background/WinEnding.png" />
            </image>
        </ImageView>

        <!-- Back to Menu Button with image -->
        <Button fx:id="backToMenuButton" layoutX="565.0" layoutY="410.0" mnemonicParsing="false" onAction="#BackToMenu" style="-fx-background-color: transparent;">
            <graphic>
                <ImageView fitHeight="79.0" fitWidth="124.0">
                    <image>
                        <Image url="@../../Graphics/Misc/menu.png" />
                    </image>
                </ImageView>
            </graphic>
        </Button>
    </children>
</AnchorPane>
